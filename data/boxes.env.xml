<Environment>
  <Robot name="Box3DRobot" file="box3d.robot.xml">
    <translation>0 0 0</translation>
  </Robot>
  
  <KinBody name="obstacle">
    <Body type="static" name="obstacle">
      <Translation> 0.2 0.25 0 </Translation>
      <Geom type="box">
        <extents> 0.05 0.07 0.03 </extents>
        <diffuseColor>0.8 0 0</diffuseColor>
      </Geom>
    </Body>
  </KinBody>
  
  <Robot name="ObstacleRobot">
		<KinBody>
		  <Body name="Obstacle" type="dynamic">
		    <Translation> 0.2 0.25 0 </Translation>
		    <Geom type="box">
		      <extents> 0.05 0.07 0.03 </extents>
		      <diffuseColor>0.8 0 0</diffuseColor>
		    </Geom>
		  </Body>
		</KinBody>
		
		<Manipulator name="obstacle">
		  <effector>Obstacle</effector>
		  <base>Obstacle</base>
		  <Translation>0 0 0</Translation>
		</Manipulator>
	</Robot>
  
</Environment>



